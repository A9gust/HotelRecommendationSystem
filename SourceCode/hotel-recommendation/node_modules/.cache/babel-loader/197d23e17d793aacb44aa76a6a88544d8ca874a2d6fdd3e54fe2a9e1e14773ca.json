gn up for Gold Membership? :\">\n        <el-radio-group v-model=\"form.goldMembership\">\n          <el-radio label=\"Yes\">Yes</el-radio>\n          <el-radio label=\"No\">No</el-radio>\n        </el-radio-group>\n      </el-form-item>\n\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"submitForm\">Submit</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      form: {\n        name: '',\n        age: '',\n        gender: '',\n        address: '',\n        country: '',\n        holidayFrequency: '', \n        goldMembership: '',   \n      },\n    };\n  },\n  methods: {\n    submitForm() {\n      this.$axios.post('http://localhost:5000/signup', this.form)\n        .then(response => {\n          console.log('User info submitted', response);\n        })\n        .catch(error => {\n          console.error('Error submitting form', error);\n        });\n    },\n  },\n};\n</script>\n\n<style>\n.user-signup {\n  width: 800px;\n  margin: 120px auto;\n  padding: 20px;\n  border: 1.5px solid #ccc;\n  border-radius: 10px;\n  background-color: #f9f9f9;\n}\n\n.el-radio {\n  font-size: 18px; \n  padding: 0px 48px; \n}\n\n\n.el-form-item {\n  display: flex;\n  align-items: center;\n  justify-content: flex-start;\n  margin-bottom: 30px;\n  margin-top: 25px;\n  width: 1000px;\n}\n\n.el-form-item__label {\n  margin-left: 25px !important;\n  text-align: left;\n  vertical-align: middle;\n  float: left;\n  font-size: 22px !important;\n  font-family: 'Times New Roman' !important;\n  color: #060606;\n  line-height: 40px;\n  padding: 0px 0px 0 0;\n  box-sizing: border-box;\n  font-weight: bold; /* 加粗标签字体 */\n  white-space: nowrap; /* 防止文字换行 */\n}\n\n.el-input__inner {\n  flex: 1;\n  font-family: 'Times New Roman', Arial, sans-serif;\n  font-size: 21px;\n  padding: 10px;\n}\n\n.el-button {\n  font-size: 18px;\n  font-weight: bold;\n  padding: 10px 20px;\n  background-color: #409eff;\n  color: