\n    </div>\r\n    <button class=\"next-button\" @click=\"goToNextPage\">Next</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      selectedOption: '',\r\n      recommendations: [\r\n        {\r\n          name: 'Hotel Sunshine',\r\n          imageUrl: 'path/to/hotel-sunshine.jpg',\r\n          details: 'A beautiful hotel located near the beach.',\r\n          rating: 4\r\n        },\r\n        {\r\n          name: 'Mountain Retreat',\r\n          imageUrl: 'path/to/mountain-retreat.jpg',\r\n          details: 'A quiet retreat in the mountains.',\r\n          rating: 5\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  mounted() {\r\n    const query = this.$route.query;\r\n    this.selectedOption = query.selectedOption || 'Your selection';\r\n    this.fetchHotelData(query.rates);\r\n  },\r\n  methods: {\r\n    async fetchHotelData(rates) {\r\n      try {\r\n        const response = await axios.get(`/api/hotels?rates=${rates}`);\r\n        this.recommendations = [...this.recommendations, ...response.data];\r\n      } catch (error) {\r\n        console.error('Error fetching hotel data:', error);\r\n      }\r\n    },\r\n    goToNextPage() {\r\n      this.$router.push('/finalpage');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.title-container {\r\n  text-align: center; /* Center the title */\r\n  margin-bottom: 10px; /* Add some spacing between the title and the border box */\r\n}\r\n\r\n.title-text {\r\n  font-size: 30px;\r\n  font-family: 'Times New Roman', Times, serif;\r\n}\r\n\r\n.border-box {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: flex-start;\r\n  width: 60%;\r\n  max-width: 800px;\r\n  overflow-y: auto;\r\n  margin: 20px auto;\r\n  padding: 20px;\r\n  background-color: #f0f0f0;\r\n  border: 2px solid #ddd;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.hotel-list {\r\n  di