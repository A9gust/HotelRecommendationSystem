{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      selectedOptions: [],\n      // 存储用户选择的选项\n      vacationOptions: [{\n        name: 'City Hotel',\n        imageUrl: require('@/assets/city-hotel.png')\n      }, {\n        name: 'Mountain View',\n        imageUrl: require('@/assets/mountain-view.png')\n      }, {\n        name: 'Beach Resort',\n        imageUrl: require('@/assets/beach-resort.png')\n      }, {\n        name: 'Glamping',\n        imageUrl: require('@/assets/glamping.png')\n      }, {\n        name: 'Tree House',\n        imageUrl: require('@/assets/tree-house.png')\n      }, {\n        name: 'Overwater Bungalow',\n        imageUrl: require('@/assets/overwater-bungalow.png')\n      }]\n    };\n  },\n  methods: {\n    submitSelection() {\n      // 将勾选框选中的内容发送到后端\n      this.$axios.post('http://localhost:5000/saveSelection', {\n        selectedOptions: this.selectedOptions\n      }).then(response => {\n        console.log('Selection saved:', response);\n        // 跳转到下一页\n        this.$router.push('/nextpage');\n      }).catch(error => {\n        console.error('Error saving selection:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","selectedOptions","vacationOptions","name","imageUrl","require","methods","submitSelection","$axios","post","then","response","console","log","$router","push","catch","error"],"sources":["src/components/UserSelection.vue"],"sourcesContent":["<template>\r\n    <div class=\"border-box\">\r\n      <div class=\"select-title\">Based on your ideal vacation you would be most interested in:</div>\r\n      <div class=\"image-grid\">\r\n        <div v-for=\"(option, index) in vacationOptions\" :key=\"index\" class=\"option-item\">\r\n          <div class=\"image-container\">\r\n            <img :src=\"option.imageUrl\" alt=\"Vacation Image\" class=\"vacation-image\">\r\n            <el-checkbox v-model=\"selectedOptions\" :label=\"option.name\" class=\"checkbox\"></el-checkbox>\r\n          </div>\r\n          <div class=\"option-name\">{{ option.name }}</div> <!-- 图片下方显示name -->\r\n        </div>\r\n      </div>\r\n      <button class=\"confirm-button\" @click=\"submitSelection\">Please confirm your selection</button>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        selectedOptions: [], // 存储用户选择的选项\r\n        vacationOptions: [\r\n          { name: 'City Hotel', imageUrl: require('@/assets/city-hotel.png') },\r\n          { name: 'Mountain View', imageUrl: require('@/assets/mountain-view.png') },\r\n          { name: 'Beach Resort', imageUrl: require('@/assets/beach-resort.png') },\r\n          { name: 'Glamping', imageUrl: require('@/assets/glamping.png') },\r\n          { name: 'Tree House', imageUrl: require('@/assets/tree-house.png') },\r\n          { name: 'Overwater Bungalow', imageUrl: require('@/assets/overwater-bungalow.png') }\r\n        ]\r\n      };\r\n    },\r\n    methods: {\r\n      submitSelection() {\r\n        // 将勾选框选中的内容发送到后端\r\n        this.$axios.post('http://localhost:5000/saveSelection', {\r\n          selectedOptions: this.selectedOptions\r\n        })\r\n        .then(response => {\r\n          console.log('Selection saved:', response);\r\n          // 跳转到下一页\r\n          this.$router.push('/nextpage');\r\n        })\r\n        .catch(error => {\r\n          console.error('Error saving selection:', error);\r\n        });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .border-box {\r\n    border: 1px solid #000;\r\n    padding: 20px;\r\n    border-radius: 5px;\r\n    width: 800px;\r\n    height: 550px;\r\n    margin: 120px auto;\r\n    padding: 20px;\r\n    box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .select-title {\r\n    font-family: 'Times New Roman', Arial, sans-serif;\r\n    font-size: 30px;\r\n    font-weight: bold;\r\n    margin-bottom: 40px;\r\n    margin-top: 20px;\r\n    text-align: center;\r\n  }\r\n  \r\n  .image-grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(3, 1fr);\r\n    gap: 20px;\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .image-container {\r\n    position: relative;\r\n    text-align: center; /* 居中显示图片 */\r\n  }\r\n  \r\n  .vacation-image {\r\n    width: 100%;\r\n    height: auto;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  .checkbox {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 10px;\r\n    transform: scale(1.5); /* 可调整复选框的大小 */\r\n  }\r\n  \r\n  .option-name {\r\n    margin-top: 10px;\r\n    text-align: center; /* name 居中显示 */\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .confirm-button {\r\n    margin-top: 20px;\r\n    padding: 10px 20px;\r\n    background-color: #fbfefc;\r\n    color: rgb(0, 0, 0);\r\n    border-radius: 5px;\r\n    font-size: 18px;\r\n    cursor: pointer;\r\n    float: right;\r\n  }\r\n  \r\n  .confirm-button:hover {\r\n    background-color: #808480;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAiBA;EACAA,KAAA;IACA;MACAC,eAAA;MAAA;MACAC,eAAA,GACA;QAAAC,IAAA;QAAAC,QAAA,EAAAC,OAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,QAAA,EAAAC,OAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,QAAA,EAAAC,OAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,QAAA,EAAAC,OAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,QAAA,EAAAC,OAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,QAAA,EAAAC,OAAA;MAAA;IAEA;EACA;EACAC,OAAA;IACAC,gBAAA;MACA;MACA,KAAAC,MAAA,CAAAC,IAAA;QACAR,eAAA,OAAAA;MACA,GACAS,IAAA,CAAAC,QAAA;QACAC,OAAA,CAAAC,GAAA,qBAAAF,QAAA;QACA;QACA,KAAAG,OAAA,CAAAC,IAAA;MACA,GACAC,KAAA,CAAAC,KAAA;QACAL,OAAA,CAAAK,KAAA,4BAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}