{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      selectedOption: '',\n      // 用户选择的选项\n      recommendations: [] // 后端返回的推荐结果\n    };\n  },\n  mounted() {\n    // 从路由的 query 中获取推荐内容\n    const query = this.$route.query;\n    this.recommendations = JSON.parse(query.rates);\n    this.selectedOption = this.$route.query.selectedOption || 'Your selection';\n  },\n  methods: {\n    goToNextPage() {\n      this.$router.push('/finalpage'); // 跳转到下一页\n    }\n  }\n};","map":{"version":3,"names":["data","selectedOption","recommendations","mounted","query","$route","JSON","parse","rates","methods","goToNextPage","$router","push"],"sources":["src/components/UserRate.vue"],"sourcesContent":["<template>\r\n    <div class=\"border-box\">\r\n      <h3 style=\"font-size: 30px; font-family: 'Times New Roman', Times, serif;\">For the selection of <span>{{ selectedOption }}</span></h3>\r\n      <div v-for=\"(rec, index) in rates\" :key=\"index\" class=\"rate-item\">\r\n        <img :src=\"rec.imageUrl\" alt=\"Hotel Image\" class=\"hotel-image\">\r\n        <div>\r\n          <h4>{{ rec.name }}</h4>\r\n          <p>{{ rec.details }}</p>\r\n          <el-rate :value=\"rec.rating\" disabled></el-rate>\r\n        </div>\r\n      </div>\r\n      <button class=\"next-button\" @click=\"goToNextPage\">Next</button>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        selectedOption: '', // 用户选择的选项\r\n        recommendations: [] // 后端返回的推荐结果\r\n      };\r\n    },\r\n    mounted() {\r\n      // 从路由的 query 中获取推荐内容\r\n      const query = this.$route.query;\r\n      this.recommendations = JSON.parse(query.rates);\r\n      this.selectedOption = this.$route.query.selectedOption || 'Your selection';\r\n    },\r\n    methods: {\r\n      goToNextPage() {\r\n        this.$router.push('/finalpage');  // 跳转到下一页\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n 